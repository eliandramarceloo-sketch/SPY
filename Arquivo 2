// Componente MainScreen
const MainScreen = ({ userData, onStartScan }) => {
  const [targetName, setTargetName] = useState('');
  const [phone, setPhone] = useState('');
  const [reason, setReason] = useState('');
  const [profilePhoto, setProfilePhoto] = useState(null);
  const [searchingPhoto, setSearchingPhoto] = useState(false);
  const [buttonHover, setButtonHover] = useState(false);

  const formatPhone = (value) => {
    const numbers = value.replace(/\D/g, '');
    if (numbers.length <= 11) {
      return numbers.replace(/(\d{2})(\d{5})(\d{4})/, '($1) $2-$3');
    }
    return value;
  };

  const handlePhoneChange = (e) => {
    const formatted = formatPhone(e.target.value);
    setPhone(formatted);
    if (e.target.value.replace(/\D/g, '').length === 11) {
      setSearchingPhoto(true);
      setTimeout(() => {
        setProfilePhoto(`https://i.pravatar.cc/150?img=${Math.floor(Math.random() * 70)}`);
        setSearchingPhoto(false);
      }, 2000);
    }
  };

  const handleClick = () => {
    if (phone.replace(/\D/g, '').length === 11) {
      onStartScan({ targetName, phone, reason, profilePhoto });
    } else {
      alert('Por favor, digite um nÃºmero de telefone vÃ¡lido com 11 dÃ­gitos');
    }
  };

  return React.createElement('div', { style: styles.mainContainer },
    React.createElement('div', { style: styles.header },
      React.createElement(Shield, { size: 32, color: "#00ff41" }),
      React.createElement('h2', { style: styles.headerTitle }, 'PAINEL DE RASTREAMENTO')
    ),
    React.createElement('div', { style: styles.mainForm },
      React.createElement('div', { style: styles.section },
        React.createElement('h3', { style: styles.sectionTitle }, 'DADOS DO ALVO'),
        searchingPhoto && React.createElement('div', { style: styles.photoSearching },
          React.createElement('div', { style: styles.spinner }),
          React.createElement('p', null, 'Buscando foto do WhatsApp...')
        ),
        profilePhoto && !searchingPhoto && React.createElement('div', { style: styles.photoResult },
          React.createElement('img', { src: profilePhoto, alt: 'Profile', style: styles.profilePhoto }),
          React.createElement('span', { style: styles.photoStatus }, 'âœ“ Perfil pÃºblico')
        ),
        React.createElement('input', {
          type: 'text',
          value: targetName,
          onChange: (e) => setTargetName(e.target.value),
          placeholder: 'Nome do Alvo (opcional)',
          style: styles.input
        }),
        React.createElement('input', {
          type: 'text',
          value: phone,
          onChange: handlePhoneChange,
          placeholder: '(00) 00000-0000',
          style: styles.input,
          maxLength: 15
        }),
        React.createElement('textarea', {
          value: reason,
          onChange: (e) => setReason(e.target.value),
          placeholder: 'Motivo do Rastreamento (opcional)',
          style: { ...styles.input, minHeight: '80px', resize: 'vertical' }
        })
      ),
      React.createElement('button', {
        onClick: handleClick,
        style: { ...styles.scanButton, ...(buttonHover ? { backgroundColor: '#ff8c5a', transform: 'scale(1.02)' } : {}) },
        onMouseEnter: () => setButtonHover(true),
        onMouseLeave: () => setButtonHover(false)
      }, 'INICIAR VARREDURA')
    )
  );
};

// Componente ScanningScreen
const ScanningScreen = ({ onComplete }) => {
  const [progress, setProgress] = useState(0);
  const [message, setMessage] = useState('Inicializando sistema...');

  const messages = [
    'Inicializando sistema...',
    'Conectando aos servidores...',
    'Localizando dispositivo...',
    'Interceptando sinais GPS...',
    'Analisando dados de localizaÃ§Ã£o...',
    'Coletando histÃ³rico de mensagens...',
    'Processando contatos recentes...',
    'Escaneando galeria de fotos...',
    'Finalizando varredura...',
    'Compilando relatÃ³rio...'
  ];

  useEffect(() => {
    const interval = setInterval(() => {
      setProgress(prev => {
        if (prev >= 100) {
          clearInterval(interval);
          setTimeout(() => onComplete(), 500);
          return 100;
        }
        return prev + 1;
      });
    }, 80);
    return () => clearInterval(interval);
  }, [onComplete]);

  useEffect(() => {
    const messageIndex = Math.floor(progress / 10);
    if (messages[messageIndex]) {
      setMessage(messages[messageIndex]);
    }
  }, [progress]);

  return React.createElement('div', { style: styles.scanningContainer },
    React.createElement('div', { style: styles.radar },
      React.createElement('div', { style: styles.radarCircle }),
      React.createElement('div', { style: styles.radarCircle }),
      React.createElement('div', { style: styles.radarCircle }),
      React.createElement('div', { style: styles.radarSweep })
    ),
    React.createElement('div', { style: styles.scanningInfo },
      React.createElement('h2', { style: styles.scanningTitle }, 'VARREDURA EM ANDAMENTO'),
      React.createElement('p', { style: styles.scanningMessage }, message),
      React.createElement('div', { style: styles.progressBar },
        React.createElement('div', { style: { ...styles.progressFill, width: `${progress}%` } })
      ),
      React.createElement('p', { style: styles.progressText }, `${progress}%`)
    )
  );
};

