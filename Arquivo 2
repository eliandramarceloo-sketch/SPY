// Sistema de Banco de Dados
const DATABASE = {
  users: [],
  targets: [],
  scans: []
};

const DB = {
  saveUser: (userData) => {
    const user = {
      id: `user_${Date.now()}`,
      phone: userData.phone,
      cidade: userData.cidade,
      createdAt: new Date().toISOString()
    };
    DATABASE.users.push(user);
    return user;
  },
  
  saveTarget: (targetData, userId) => {
    const target = {
      id: `target_${Date.now()}`,
      userId: userId,
      targetName: targetData.targetName,
      phone: targetData.phone,
      reason: targetData.reason,
      profilePhoto: targetData.profilePhoto,
      createdAt: new Date().toISOString()
    };
    DATABASE.targets.push(target);
    return target;
  },
  
  saveScan: (userId, targetId) => {
    const scan = {
      id: `scan_${Date.now()}`,
      userId: userId,
      targetId: targetId,
      status: 'completed',
      createdAt: new Date().toISOString()
    };
    DATABASE.scans.push(scan);
    return scan;
  }
};

// Componente LoginScreen
const LoginScreen = ({ onLogin }) => {
  const [phone, setPhone] = useState('');
  const [cidade, setCidade] = useState('');
  const [errors, setErrors] = useState({});
  const [buttonHover, setButtonHover] = useState(false);

  const formatPhone = (value) => {
    const numbers = value.replace(/\D/g, '');
    if (numbers.length <= 11) {
      return numbers.replace(/(\d{2})(\d{5})(\d{4})/, '($1) $2-$3');
    }
    return value;
  };

  const handlePhoneChange = (e) => {
    const formatted = formatPhone(e.target.value);
    setPhone(formatted);
  };

  const handleClick = () => {
    const newErrors = {};
    if (!phone || phone.replace(/\D/g, '').length !== 11) {
      newErrors.phone = 'Telefone vÃ¡lido com 11 dÃ­gitos Ã© obrigatÃ³rio';
    }
    if (!cidade) {
      newErrors.cidade = 'Cidade Ã© obrigatÃ³ria';
    }
    if (Object.keys(newErrors).length > 0) {
      setErrors(newErrors);
      return;
    }
    onLogin({ phone, cidade, timestamp: new Date().toISOString() });
  };

  return React.createElement('div', { style: styles.loginContainer },
    React.createElement('div', { style: styles.loginBox },
      React.createElement('div', { style: styles.logo },
        React.createElement(Shield, { size: 48, color: "#00ff41" }),
        React.createElement('h1', { style: styles.title }, 'SPY ON WATTS'),
        React.createElement('p', { style: styles.subtitle }, 'Sistema de Rastreamento AvanÃ§ado')
      ),
