// App Principal
function App() {
  const [screen, setScreen] = useState('login');
  const [userData, setUserData] = useState(null);
  const [targetData, setTargetData] = useState(null);
  const [currentUserId, setCurrentUserId] = useState(null);
  const [currentTargetId, setCurrentTargetId] = useState(null);

  const handleLogin = (data) => {
    const user = DB.saveUser(data);
    setCurrentUserId(user.id);
    setUserData(data);
    setScreen('main');
  };

  const handleStartScan = (data) => {
    const target = DB.saveTarget(data, currentUserId);
    setCurrentTargetId(target.id);
    setTargetData(data);
    setScreen('scanning');
  };

  const handleScanComplete = () => {
    DB.saveScan(currentUserId, currentTargetId);
    setScreen('results');
  };

  return React.createElement('div', { style: styles.app },
    screen === 'login' && React.createElement(LoginScreen, { onLogin: handleLogin }),
    screen === 'main' && userData && React.createElement(MainScreen, { userData, onStartScan: handleStartScan }),
    screen === 'scanning' && React.createElement(ScanningScreen, { onComplete: handleScanComplete }),
    screen === 'results' && userData && targetData && React.createElement(ResultsScreen, { userData, targetData })
  );
}

