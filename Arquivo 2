        React.createElement('div', { style: styles.inputGroup },
          React.createElement('label', { style: styles.label }, 'Telefone'),
          React.createElement('input', {
            type: 'tel',
            value: phone,
            onChange: handlePhoneChange,
            style: styles.input,
            placeholder: '(00) 00000-0000',
            maxLength: 15
          }),
          errors.phone && React.createElement('span', { style: styles.error }, errors.phone)
        ),
        React.createElement('div', { style: styles.inputGroup },
          React.createElement('label', { style: styles.label }, 'Cidade'),
          React.createElement('select', {
            value: cidade,
            onChange: (e) => setCidade(e.target.value),
            style: styles.input
          },
            React.createElement('option', { value: '' }, 'Selecione sua cidade'),
            React.createElement('option', { value: 'Erechim' }, 'Erechim - RS'),
            React.createElement('option', { value: 'Passo Fundo' }, 'Passo Fundo - RS'),
            React.createElement('option', { value: 'Caxias do Sul' }, 'Caxias do Sul - RS'),
            React.createElement('option', { value: 'Porto Alegre' }, 'Porto Alegre - RS'),
            React.createElement('option', { value: 'SÃ£o Paulo' }, 'SÃ£o Paulo - SP'),
            React.createElement('option', { value: 'Rio de Janeiro' }, 'Rio de Janeiro - RJ'),
            React.createElement('option', { value: 'Belo Horizonte' }, 'Belo Horizonte - MG'),
            React.createElement('option', { value: 'Curitiba' }, 'Curitiba - PR'),
            React.createElement('option', { value: 'FlorianÃ³polis' }, 'FlorianÃ³polis - SC'),
            React.createElement('option', { value: 'Salvador' }, 'Salvador - BA'),
            React.createElement('option', { value: 'Fortaleza' }, 'Fortaleza - CE'),
            React.createElement('option', { value: 'BrasÃ­lia' }, 'BrasÃ­lia - DF')
          ),
          errors.cidade && React.createElement('span', { style: styles.error }, errors.cidade)
        ),
        React.createElement('button', {
          onClick: handleClick,
          style: { ...styles.submitButton, ...(buttonHover ? { backgroundColor: '#00cc33', boxShadow: '0 0 20px rgba(0, 255, 65, 0.6)' } : {}) },
          onMouseEnter: () => setButtonHover(true),
          onMouseLeave: () => setButtonHover(false)
        }, 'INICIAR RASTREAMENTO')
      ),
      React.createElement('div', { style: styles.disclaimer },
        React.createElement(AlertTriangle, { size: 16, color: "#ff6b35" }),
        React.createElement('p', { style: styles.disclaimerText }, 'ã“ã‚Œã¯æ•™è‚²ç›®çš„ã®ã‚·ãƒŸãƒ¥ãƒ¬ãƒ¼ã‚·ãƒ§ãƒ³ã‚¢ãƒ—ãƒªã‚±ãƒ¼ã‚·ãƒ§ãƒ³ã§ã™ã€‚å®Ÿéš›ã®ãƒ‡ãƒ¼ã‚¿ã¯åŽé›†ã¾ãŸã¯è¿½è·¡ã•ã‚Œã¾ã›ã‚“ã€‚')
